<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parkinglot">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ParkingLot"
        android:usesCleartextTraffic="true">
        <activity android:name=".ParkingRecordActivity" android:parentActivityName=".ParkingActivity_Main"></activity>
        <activity
            android:name=".ParkingDetailsActivity"
            android:parentActivityName=".ParkingActivity_Main" />
        <activity android:name=".ParkingActivity_Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>